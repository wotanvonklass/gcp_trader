# Firehose Proxy Connection
FIREHOSE_URL=ws://localhost:8767

# Polygon API Key (for authentication)
POLYGON_API_KEY=your_polygon_api_key_here

# Ms-Aggregator WebSocket Server Port
AGGREGATOR_PORT=8768

# Interval Limits (milliseconds)
MIN_INTERVAL_MS=1
MAX_INTERVAL_MS=60000

# Timer Settings
TIMER_INTERVAL_MS=10    # How often to check for completed bars
BAR_DELAY_MS=20         # Delay before emitting bars (to catch late trades)

# Logging
LOG_LEVEL=info
